@Library('jenkins-groovy-library-script') _

def repoName = "poc-react-backend-parent"
def repoUrl = "https://github.com/siukatech/poc-react-backend-parent.git"
def repoHttp = "https"
def repoHostname = "github.com"
def repoUsername = "siukatech"
gradlePublishAndTag(
    "repoName": "${repoName}"
    , "repoUrl": "${repoUrl}"
    , "repoHttp": "${repoHttp}"
    , "repoHostname": "${repoHostname}"
    , "repoUsername": "${repoUsername}"
);

